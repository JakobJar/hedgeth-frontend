// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Fund {
  address         Bytes    @id
  name            String
  description     String?
  raisingClose    DateTime?
  close           DateTime?
}

model Token {
  address         Bytes    @id
  name            String
  symbol          String
  decimals        Int
}
